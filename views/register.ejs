 <!-- Registration  Container -->

  <!-- ..................................................................................................................................... -->
  <div class="container" id="RegistrationContainer">
    <div class="row"></div>
    <div class="row"></div>
    <div id="login-page" class="row">
        
      <div class="col s12 m2 l2"> 
        </div>
      
        <div class="col s12 m8 l8 z-depth-4 card-panel">
        <form class="login-form">
          <div class="row">
            <div class="input-field col s12 center">
              <h3 class="center " style="font-family:Streetwear;text-shadow: 0px 2px 2px black;  color:#1dc072">Register</h3>
            </div>
          </div>
          
          <div class="row margin">
            <div class="input-field col s12 m6 l6">
              <i class="material-icons prefix">account_circle</i>
              <input id="fname" type="text">
              <label for="fname" class="center-align">First Name</label>
            </div>
            <div class="input-field col s12 m6 l6">
              <i class="material-icons prefix">account_circle</i>
              <input id="lname" type="text">
              <label for="lname" class="center-align">Last Name</label>
            </div>
          </div>
        

          <div class="row">
              <div class="col s12 l6 m12 ">
                  <div class="row margin">
                      <div class="input-field col s12">
                        <i class="material-icons prefix">account_circle</i>
                        <input id="email" type="text">
                        <label for="email" class="center-align">Email</label>
                      </div>
                    </div>


                    <!-- You May Want to Change the Id on these two -->

                    <!-- ................................................... -->
          <div class="row margin">
            <div class="input-field col s12">
              <i class="material-icons prefix">account_circle</i>
              <input id="username" type="text">
              <label for="username" class="center-align">Username</label>
            </div>
          </div>
          <div class="row margin center-align">
            <div class="input-field col s12">
              <i class="material-icons prefix">lock</i>
              <input id="password" type="password">
              <label for="password">Password</label>
            </div>
          </div>
                    <!-- ................................................... -->

          <div class="row margin center-align">
            <div class="input-field col s12">
              <i class="material-icons prefix">lock</i>
              <input id="repassword" type="password">
              <label for="repassword">Verify Password</label>
            </div>
          </div>

          <div class="row margin">
            <div class="input-field col s12">
              <i class="material-icons prefix">account_circle</i>
              <input id="dob" type="text" class="datepicker">
              <label for="dob" class="center-align">Date of Birth</label>
            </div>
          </div>
        </div>

        <div class="col s6 hide-on-med-and-down">
          <img src="images/reg.png" style="width: 90%;" alt="NA">

        </div>

      </div>

          <div class="row">
            <div class="col s3"></div>
            <div class="input-field col s6  ">
              <a href="/registerDetails" style="background-color:#1dc072" class="btn waves-effect waves-light col s12 center">Register Now</a>
            </div>
            <div class="col s3"></div>
            
          </div>
          <div class="row">
            <div class="input-field col s6 m6 l6">
              <p class="margin medium-small"><a href="/Login">Already a Member</a></p>
            </div>
                 
          </div>
  
        </form>
      </div>
  
      <div class="col s12 m2 l2"> <!-- Note that "m4 l3" was added -->
  
      </div>
    </div>
  
  </div>
    <!-- ..................................................................................................................................... -->
  



  <!--  Scripts-->
  <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
